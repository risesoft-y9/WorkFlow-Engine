{
    "serverUrl": "https://dev.youshengyun.com/flowableUI",
    //设置服务器地址,非必须
    "isStrict": false,
    //是否开启严格模式
    "allInOne": true,
    //是否将文档合并到一个文件中，一般推荐为true
    "outPath": "../../../y9-digitalbase-pro/docs/smart-docs/flowableUI-position",
    //指定文档的输出路径
    "coverOld": true,
    //是否覆盖旧的文件，主要用于mardown文件覆盖
    "packageFilters": "net.risesoft.controller.mobile.v1",
    //controller包过滤，多个包用英文逗号隔开
    "md5EncryptedHtmlName": false,
    //只有每个controller生成一个html文件是才使用
    "projectName": "办件",
    "allInOneDocFileName": "index.html",
    "requestExample": "true",
    "responseExample": "true",
    //配置自己的项目名称
    "skipTransientField": true,
    //目前未实现
    "showAuthor": true,
    //是否显示接口作者名称，默认是true,不想显示可关闭
    "requestFieldToUnderline": false,
    //自动将驼峰入参字段在文档中转为下划线格式,//@since 1.8.7版本开始
    "responseFieldToUnderline": false,
    //自动将驼峰入参字段在文档中转为下划线格式,//@since 1.8.7版本开始
    "dataDictionaries": [
        //配置数据字典，没有需求可以不设置
        {
            "title": "http状态码字典",
            //数据字典的名称
            "enumClassName": "com.power.common.enums.HttpCodeEnum",
            //数据字典枚举类名称
            "codeField": "code",
            //数据字典字典码对应的字段名称
            "descField": "message"
            //数据字典对象的描述信息字典
        }
    ],
    "errorCodeDictionaries": [
        {
            //错误码列表，没有需求可以不设置
            "enumClassName": "net.risesoft.exception.ErrorCode",
            //错误码枚举类
            "codeField": "code",
            //错误码的code码字段名称
            "descField": "description"
            //错误码的描述信息对应的字段名
        }
    ],
    "revisionLogs": [
        //设置文档变更记录，没有需求可以不设置
        {
            "version": "1.0",
            //文档版本号
            "revisionTime": "2020/6/10",
            "status": "create",
            //变更操作状态，一般为：创建、更新等
            "author": "zhangchongjie",
            //文档变更作者
            "remarks": "第一版办件接口文档"
            //变更描述
        }
    ],
    "customResponseFields": [
        //自定义添加字段和注释，api-doc后期遇到同名字段则直接给相应字段加注释，非必须
        {
            "name": "code",
            //覆盖响应码字段
            "desc": "响应代码",
            //覆盖响应码的字段注释
            "value": "00000"
            //设置响应码的值
        }
    ],
    "requestHeaders": [
        //设置请求头，没有需求可以不设置
    ],
    "rpcApiDependencies": [
        {
            // 项目开放的dubbo api接口模块依赖，配置后输出到文档方便使用者集成
            "artifactId": "SpringBoot2-Dubbo-Api",
            "groupId": "com.demo",
            "version": "1.0.0"
        }
    ],
    "rpcConsumerConfig": "src/main/resources/consumer-example.conf",
    //文档中添加dubbo consumer集成配置，用于方便集成方可以快速集成
    "apiObjectReplacements": [
        {
            // 自smart-doc 1.8.5开始你可以使用自定义类覆盖其他类做文档渲染，非必须
            "className": "org.springframework.data.domain.Pageable",
            "replacementClassName": "com.power.doc.model.PageRequestDto"
            //自定义的PageRequestDto替换Pageable做文档渲染
        }
    ],
    "sourceCodePaths": [
        //设置代码路径, 插件已经能够自动下载发布的源码包，没必要配置
        {
            "path": "src/main/java",
            "desc": "测试"
        }
    ]
}